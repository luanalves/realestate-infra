# Code path
APP_PATH=

# Database Connection data
DB_CONNECTION=
DB_HOST=
DB_PORT=
DB_DATABASE=
DB_USERNAME=
DB_PASSWORD=

# Redis
REDIS_HOST=
REDIS_PORT=

# MongoDB
MONGO_DB_HOST=
MONGO_DB_PORT=
# MongoDB database name used by the application
MONGO_DB_DATABASE=
# MongoDB connection URI (optional, overrides host/port/database)
MONGO_URI=

# Laravel Application Environment
APP_ENV=
APP_DEBUG=

# Xdebug Configuration (for development)
# Set to 1 to enable Xdebug, 0 to disable
XDEBUG_MODE=debug,develop,coverage
XDEBUG_CLIENT_HOST=host.docker.internal
XDEBUG_CLIENT_PORT=9004